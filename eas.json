{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API": "https://sp-slidtodo-api.vercel.app",
        "EXPO_PUBLIC_API_TEAMID": "team555"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_API": "https://sp-slidtodo-api.vercel.app",
          "EXPO_PUBLIC_API_TEAMID": "team555"
        }
      }
    },
    "production": {
      "autoIncrement": true
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
