{
  "semi": false,
  "tabWidth": 2,
  "singleQuote": false,
  "printWidth": 100,
  "bracketSpacing": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",

    "^@assets/(.*)$",
    "^@components/(.*)$",
    "^@apis/(.*)$",
    "^@hooks/(.*)$",
    "^@constant/(.*)$",
    "^@types/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
